a = 3
b = 5
resultado = "a x b"


print("Executando uma conta matemática")
print(resultado)
print("a=3 b=5")
print("Resultado:")
print(a*b)

print("Próximo exercício Questão 5")

num1 =10
num2 =5
num3 =8
media = (num1+num2+num3)

print("Soma dos valores: num1=10 + num2=5 + num3=8")
print("Resultado:")
print(media)

print("Agora exiba a media...")
print("Resultado:")

print(media/3)



